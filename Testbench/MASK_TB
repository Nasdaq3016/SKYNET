module MASK_TB ();

  reg [255:0] data_in;
  reg [255:0] data_out;
  
  MASK dut (
    .data_in(data_in),
    .data_out(data_out)
  );


  initial begin
    data_in = 16'b5c1aee973812059b199c7001769dba960c1af2176a199d997817f608c18e5816;

    $display("data_out = %h",data_out);
    $display("정답: %b", Y == 16'b5c1aee973812059b1111110000000000199c7001769d111111000000000011111100000000000c1af2171111110000000000111111000000000011111100000000005816);
    $finish;
  end
  
endmodule
