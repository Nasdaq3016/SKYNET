module dsp_recip_sprt(
input clk,
input s_axis_a_tvalid,
output s_axis_a_tready,
input [15:0] data_a,
output m_axis_result_tvalid,
input m_axis_result_tready,
output [15:0] data_out
    );
  Reciporcal_Sqrt_IP recip_sqrt (
  .aclk(clk),                                   // input wire aclk
  .s_axis_a_tvalid(s_axis_a_tvalid),            // input wire s_axis_a_tvalid
  .s_axis_a_tready(s_axis_a_tready),            // output wire s_axis_a_tready
  .s_axis_a_tdata(data_a),                      // input wire [15 : 0] s_axis_a_tdata
  .m_axis_result_tvalid(m_axis_result_tvalid),  // output wire m_axis_result_tvalid
  .m_axis_result_tready(m_axis_result_tready),  // input wire m_axis_result_tready
  .m_axis_result_tdata(data_out)                // output wire [15 : 0] m_axis_result_tdata
);
endmodule
